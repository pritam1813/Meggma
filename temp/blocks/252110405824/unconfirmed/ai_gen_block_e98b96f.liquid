        {% endif %}
      </div>
    </div>

    <div class="ai-announcement-bar__right-{{ ai_gen_id }}">
      {% if block.settings.show_about and block.settings.about_url != blank %}
        <a href="{{ block.settings.about_url }}" class="ai-announcement-bar__nav-link-{{ ai_gen_id }}">
          {{ block.settings.about_text }}
        </a>
      {% endif %}

