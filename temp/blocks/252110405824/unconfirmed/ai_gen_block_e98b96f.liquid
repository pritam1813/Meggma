{% doc %}
  @prompt
    Announcements bar features required ,  This includes the options to add left side of the bar should have help center find store right side should have about social media accounts
{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-announcement-bar-{{ ai_gen_id }} {
    display: block;
    width: 100%;
    background-color: {{ block.settings.background_color }};
    color: {{ block.settings.text_color }};
    position: relative;
    overflow: hidden;
  }

  .ai-announcement-bar__wrapper-{{ ai_gen_id }} {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: {{ block.settings.vertical_padding }}px {{ block.settings.horizontal_padding }}px;
    max-width: {{ block.settings.max_width }}px;
    margin: 0 auto;
    gap: 20px;
  }

  .ai-announcement-bar__left-{{ ai_gen_id }},
  .ai-announcement-bar__right-{{ ai_gen_id }} {
    display: flex;
    align-items: center;
    gap: 16px;
    flex-shrink: 0;
  }

  .ai-announcement-bar__nav-link-{{ ai_gen_id }} {
    color: {{ block.settings.text_color }};
    text-decoration: none;
    font-size: {{ block.settings.nav_font_size }}px;
    white-space: nowrap;
    transition: opacity 0.2s ease;
  }

  .ai-announcement-bar__nav-link-{{ ai_gen_id }}:hover {
    opacity: 0.7;
  }

  .ai-announcement-bar__content-{{ ai_gen_id }} {
    display: flex;
    align-items: center;
    justify-content: center;
    gap: 12px;
    flex: 1;
    text-align: center;
    min-width: 0;
  }

  .ai-announcement-bar__icon-{{ ai_gen_id }} {
    display: flex;
    align-items: center;
    flex-shrink: 0;
  }

  .ai-announcement-bar__icon-{{ ai_gen_id }} svg {
    width: {{ block.settings.icon_size }}px;
    height: {{ block.settings.icon_size }}px;
    fill: currentColor;
  }

  .ai-announcement-bar__text-{{ ai_gen_id }} {
    font-size: {{ block.settings.font_size }}px;
    line-height: 1.4;
    font-weight: {{ block.settings.font_weight }};
  }

  .ai-announcement-bar__link-{{ ai_gen_id }} {
    color: {{ block.settings.link_color }};
    text-decoration: underline;
    font-weight: 600;
    margin-left: 8px;
  }

  .ai-announcement-bar__link-{{ ai_gen_id }}:hover {
    color: {{ block.settings.link_hover_color }};
  }

  .ai-announcement-bar__social-{{ ai_gen_id }} {
    display: flex;
    align-items: center;
    gap: 12px;
  }

  .ai-announcement-bar__social-link-{{ ai_gen_id }} {
    color: {{ block.settings.text_color }};
    display: flex;
    align-items: center;
    transition: opacity 0.2s ease;
  }

  .ai-announcement-bar__social-link-{{ ai_gen_id }}:hover {
    opacity: 0.7;
  }

  .ai-announcement-bar__social-link-{{ ai_gen_id }} svg {
    width: {{ block.settings.social_icon_size }}px;
    height: {{ block.settings.social_icon_size }}px;
    fill: currentColor;
  }

  .ai-announcement-bar__close-{{ ai_gen_id }} {
    background: none;
    border: none;
    color: {{ block.settings.text_color }};
    cursor: pointer;
    padding: 8px;
    display: flex;
    align-items: center;
    opacity: 0.7;
    transition: opacity 0.2s ease;
    flex-shrink: 0;
  }

  .ai-announcement-bar__close-{{ ai_gen_id }}:hover {
    opacity: 1;
  }

  .ai-announcement-bar__close-{{ ai_gen_id }} svg {
    width: 16px;
    height: 16px;
    fill: currentColor;
  }

  .ai-announcement-bar-{{ ai_gen_id }}.hidden {
    display: none;
  }

  @media screen and (max-width: 989px) {
    .ai-announcement-bar__wrapper-{{ ai_gen_id }} {
      gap: 12px;
    }

    .ai-announcement-bar__left-{{ ai_gen_id }},
    .ai-announcement-bar__right-{{ ai_gen_id }} {
      gap: 12px;
    }

    .ai-announcement-bar__nav-link-{{ ai_gen_id }} {
      font-size: {{ block.settings.nav_font_size | times: 0.9 }}px;
    }
  }

  @media screen and (max-width: 749px) {
    .ai-announcement-bar__wrapper-{{ ai_gen_id }} {
      flex-wrap: wrap;
      padding: {{ block.settings.vertical_padding | times: 0.8 }}px {{ block.settings.horizontal_padding | times: 0.8 }}px;
      gap: 8px;
    }

    .ai-announcement-bar__left-{{ ai_gen_id }},
    .ai-announcement-bar__right-{{ ai_gen_id }} {
      display: none;
    }

    .ai-announcement-bar__content-{{ ai_gen_id }} {
      flex: 1 1 100%;
      order: 1;
    }

    .ai-announcement-bar__text-{{ ai_gen_id }} {
      font-size: {{ block.settings.font_size | times: 0.9 }}px;
    }

    .ai-announcement-bar__icon-{{ ai_gen_id }} svg {
      width: {{ block.settings.icon_size | times: 0.8 }}px;
      height: {{ block.settings.icon_size | times: 0.8 }}px;
    }

    .ai-announcement-bar__close-{{ ai_gen_id }} {
      order: 2;
    }
  }
{% endstyle %}

